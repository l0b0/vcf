# Autocomplete for filterous command
_vcf()
{
    local cur prev opts base
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"

    # Basic options
    opts="-h --help"

    COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
}
complete -o dirnames -o default -F _vcf vcf
